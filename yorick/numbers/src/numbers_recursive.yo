func fibonacci(n) {
  if(n <= 0){
    return 0;
  } else if(n == 1){
    return 1
  } else {
    return fibonacci(n - 1) + fibonacci(n - 2);
  }
}

func factorial(n) {
  if(n <= 1) {
    return 1;
  } else {
    return n * factorial(n - 1);
  }
}

func sum_numbers(n) {
  if(n <= 0) {
    return 0;
  } else {
    return n + sum_numbers(n - 1);
  }
}