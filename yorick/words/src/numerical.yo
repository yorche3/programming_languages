func fibonacci(n) {
	if (n < 0) {
		return -1
	} else if (n < 2) {
		return n
	}
	acc2 = 0
	acc1 = 1
	aux = 0
	while (n > 2) {
		aux = acc1 + acc2
		acc2 = acc1
		acc1 = aux
		n--
	}
	return acc1 + acc2
}

func factorial(n) {
 	if (n < 0) {
  	return -1;
 	}
 	acc = 1
 	while (n > 1) {
   	acc *= n--
 	}
 	return acc
}

func gcd(a, b) {
	aux = 0
 	while (b != 0) {
  	aux = a % b
  	a = b
  	b = aux
 	}
 	return a
}

func lcm(a, b) {
 	return (a / gcd(a, b)) * b
}