use Test;

use lib 'lib';
use Numerical;

plan 12;

is(fibonacci(-3), -1, 'fibonacci 1');
is(fibonacci(10), 55, 'fibonacci 2');
is(fibonacci(15), 610, 'fibonacci 3');

is(factorial(-3), -1, 'factorial 1');
is(factorial(5), 120, 'factorial 2');
is(factorial(10), 3628800, 'factorial 3');

is(gcd(48, 18), 6, 'gcd 1');
is(gcd(100, 25), 25, 'gcd 2');
is(gcd(17, 13), 1, 'gcd 3');

is(lcm(6, 4), 12, 'lcm 1');
is(lcm(21, 6), 42, 'lcm 2');
is(lcm(7, 3), 21, 'lcm 3');