fibonacci(0, 0).
fibonacci(1, 1).
fibonacci(N, Result) :-
    N > 1,
    N1 is N -1,
    N2 is N -2,
    fibonacci(N1, R1),
    fibonacci(N2, R2),
    Result is R1 + R2.

factorial(0, 1).
factorial(N, Result) :-
    N > 0,
    N1 is N - 1,
    factorial(N1, R),
    Result is N * R.

sum_numbers(0, 0).
sum_numbers(N, Sum) :-
    N > 0,
    N1 is N - 1,
    sum_numbers(N1, R),
    Sum is R + N.