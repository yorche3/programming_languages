%% @title Welcome to the `frob' application!
%% @doc `frob' is a highly advanced frobnicator with low latency,

-module(cadenas).
-export([start/0, palindromo/1]).

palindromo(W) ->
   if 
      W -> 
         io:fwrite("True"); 
      true -> 
         io:fwrite("False") 
   end,
   1.

start() ->
   io:format("Es palindromo: ~p\n", [palindromo(true)]).