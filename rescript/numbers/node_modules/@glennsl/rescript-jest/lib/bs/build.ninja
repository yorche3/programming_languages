rescript = 1
rule astj
  command = /home/vubuntu/programming_languages/rescript/numbers/node_modules/bs-platform/linux/bsc.exe  -w a -bs-v 9.0.2 -bs-no-version-header -bs-super-errors -absname -bs-ast -o $out $i
o src/jest.ast : astj ../../src/jest.res
rule deps
  command = /home/vubuntu/programming_languages/rescript/numbers/node_modules/bs-platform/linux/bsb_helper.exe -hash 81b1b54be979df8fb025dc2929a51135 $in
  restat = 1
o src/jest.d : deps src/jest.ast src/jest.iast
o src/jest.iast : astj ../../src/jest.resi
rule mi
  command = /home/vubuntu/programming_languages/rescript/numbers/node_modules/bs-platform/linux/bsc.exe -I src -bs-no-version-header -bs-super-errors  -w a $i
  dyndep = 1
  restat = 1
o src/jest.cmi : mi src/jest.iast
rule mj
  command = /home/vubuntu/programming_languages/rescript/numbers/node_modules/bs-platform/linux/bsc.exe -bs-read-cmi -I src -bs-no-version-header -bs-super-errors  -w a -bs-package-name @glennsl/rescript-jest -bs-package-output commonjs:$in_d:.bs.js $i
  dyndep = 1
  restat = 1
o src/jest.cmj ../../src/jest.bs.js : mj src/jest.ast src/jest.cmi
